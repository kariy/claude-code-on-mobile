{
  "name": "cc-sdk-test",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev:manager": "bun run services/cc-manager/src/main.ts",
    "dev:manager:public": "bash scripts/start-server.sh",
    "dev:web": "open http://localhost:${CC_MANAGER_PORT:-8787} && bun run services/cc-manager/src/main.ts",
    "dev:cli": "bun run index.ts",
    "dev:legacy-server": "bun run server.ts",
    "test:manager": "bun test services/cc-manager/src",
    "ios:build": "bash scripts/ios-build.sh",
    "ios:install": "bash scripts/ios-install.sh",
    "ios:build-install": "bash scripts/ios-build-install.sh",
    "dev:web-ui": "cd services/cc-manager/web && bunx --bun vite",
    "build:web-ui": "cd services/cc-manager/web && bunx --bun vite build",
    "manager:clean": "rm -f ${CC_MANAGER_DB_PATH:-$HOME/.cc-manager/manager.db} && echo 'Database removed.'"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.39",
    "zod": "^4.3.6"
  }
}
